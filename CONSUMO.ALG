Algoritmo "consumo"

Var

   distancia, combustivel, consumo : real

Inicio

   escreva("Qual foi a distância percorrida? ")
   leia(distancia)
   escreva("Quanto de combustível foi gasto? ")
   leia(combustivel)

   consumo <- distancia/combustivel

   escreval("O consumo médio foi de ", consumo:8:3)


Fimalgoritmo